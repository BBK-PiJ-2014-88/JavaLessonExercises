package exercises;

import java.util.Arrays;
import java.util.List;
import java.util.stream.Stream;

public class QuestionFive {
	public static void main(String[] args) {
		new QuestionFive().launch();
	}
	public void launch(){
		List<String> words = Arrays.asList("Hello", "Bonjour", "engine", "Hurray", "What", "Dog", "boat");
		Stream<String> filteredStream = words.stream().filter(s -> Character.isUpperCase(s.charAt(0))).filter(s -> s.length() < 4).filter(s -> s.contains("e"));
	}
}
